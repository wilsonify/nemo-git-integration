[Nemo Action]
Name=Git Clone
Comment=Clone a git repository into the current directory
Exec=bash -c "REPO_URL=$(zenity --entry --text 'Enter repository URL to clone' --title 'Git Clone') && if [ -n \"$REPO_URL\" ]; then cd %P && git clone \"$REPO_URL\"; fi"
Icon-Name=git-clone
Selection=any
Extensions=dir
